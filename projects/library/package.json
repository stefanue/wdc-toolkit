{
    "name": "@10up/component-accordion",
    "version": "2.1.5",
    "author": "10up",
    "private": true,
    "description": "Accessible accordion component.",
    "main": "dist/index.js",
    "umd:main": "dist/index.umd.js",
    "source": "src/index.js",
    "style": "./dist/index.css",
    "scripts": {
        "test": "npm run build && wdc-toolkit test-unit-jest",
        "start": "wdc-toolkit start -i=src/app.js --dev-server",
        "lint": "wdc-toolkit lint-js",
        "build": "npm run build:modern && npm run build:umd",
        "build:modern": "wdc-toolkit build -f=commonjs --wp=false",
        "build:umd": "wdc-toolkit build -f=umd -i=src/index.umd.js --name=TenUpAccordion --external=none"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/10up/component-accordion.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/10up/component-accordion/issues"
    },
    "homepage": "https://github.com/10up/component-accordion#readme",
    "devDependencies": {
        "wdc-toolkit": "^5.2.2",
        "@testing-library/dom": "^7.29.4",
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/user-event": "^12.6.3",
        "jest-axe": "^4.1.0"
    },
    "dependencies": {
        "xss": "1.0.11"
    },
    "wdc-toolkit": {
        "libraryName": "TenUpAccordion"
    }
}
