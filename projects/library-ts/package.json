{
    "name": "@wdc/library-ts-test",
    "version": "2.1.5",
    "private": true,
    "author": "Stefan Vasiljevic",
    "description": "Accessible accordion component.",
    "main": "dist/index.js",
    "source": "src/index.ts",
    "style": "./dist/index.css",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/mjs/index.js"
        },
        "./config": {
            "require": "./dist/cjs/config.js",
            "import": "./dist/mjs/config.js"
        },
        "./util": {
            "require": "./dist/cjs/util.js",
            "import": "./dist/mjs/util.js"
        }
    },
    "scripts": {
        "watch": "wdc-toolkit build -- --watch",
        "build": "npm run build:mjs && npm run build:cjs",
        "build:mjs": "wdc-toolkit build --format=module -o=dist/mjs",
        "build:cjs": "wdc-toolkit build -o=dist/cjs",
        "format": "wdc-toolkit format-js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/10up/component-accordion.git"
    },
    "license": "GPL-2.0-or-later",
    "bugs": {
        "url": "https://github.com/10up/component-accordion/issues"
    },
    "homepage": "https://github.com/10up/component-accordion#readme",
    "devDependencies": {
        "wdc-toolkit": "^5.2.2"
    },
    "dependencies": {
        "xss": "^1.0.11"
    },
    "wdc-toolkit": {
        "libraryName": "TenUpAccordion",
        "entry": {
            "index": "./src/index.ts",
            "config": "./src/config/inde.ts",
            "util": "./src/util/index.ts"
        }
    }
}
